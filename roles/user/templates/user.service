[Unit]
Description = User Service
[Service]
User=roboshop
Environment=DOCUEMENTDB=true
Environment=REDIS_HOST=redis-dev.kiranprav.link
Environment=MONGO_URL=MONGO_URL={{ lookup('aws_ssm', "{{env}}.{{component}}.DOCDB_URL", region='us-east-1' ) }}"
ExecStart=/bin/node /app/server.js
SyslogIdentifier=user

[Install]
WantedBy=multi-user.target